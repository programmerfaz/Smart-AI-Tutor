{
  "expo": {
    "name": "my-expo-app",
    "slug": "my-expo-app",
    "scheme": "myexpoapp",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-web-browser"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fazil-programmer.myexpoapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "googleServicesFile": "./google-services.json" ,
        "permissions": [
          "RECORD_AUDIO"
        ]
      },
      "package": "com.fazil_programmer.myexpoapp"
    },
    "extra": {
      "eas": {
        "projectId": "7d25f903-3e04-42b4-9644-ae4ecb5a1ab9"
      }
    }
  }
}
